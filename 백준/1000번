//백준 1000번 문제
//람다식을 사용하여 a+b 
import java.util.Scanner;

@FunctionalInterface
interface MyInterface{
    public int method(int a, int b);

}
public class TEST {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        MyInterface fi;
        fi=(a, b) -> {
            int c= a+b;
            return c;
        };
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c=fi.method(a,b);

        System.out.println(c);
    }
}
